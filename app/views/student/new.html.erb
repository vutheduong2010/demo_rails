<h1>Them sinh vien moi</h1>
<%= form_with(model: @student, url: student_index_path,  method: :post, local: true) do |form| %>
  <div class="field">
    <%= form.label :student_code %>
    <%= form.text_field :student_code %>
  </div>

  <div class="field">
    <%= form.label :student_name %>
    <%= form.text_field :student_name %>
  </div>

  <div class="field">
    <%= form.label :date_of_birth %>
    <%= form.text_field :date_of_birth %>
  </div>

  <div class="field">
    <%= form.label :address %>
    <%= form.text_field :address %>
  </div>

  <div class="field">
    <%= form.label :gmail %>
    <%= form.text_field :gmail %>
  </div>

  <div class="field">
    <%= form.label :phone %>
    <%= form.text_field :phone %>
  </div>

  <%= form.submit 'Save' %>
<% end %>
<%= link_to 'Quay lai', student_index_path %>

