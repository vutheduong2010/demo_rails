<h1>Edit information students</h1>
<%= render 'form' %>
<%= link_to 'Xem chi tiet', @student %> |
<%= link_to 'Student list', student_path %>
<form>
<%= form_with(model: @student, local: true) do |form| %>
  <div class="field">
    <%= form.label :student_code %>
    <%= form.text_field :student_code %>
  </div>

  <div class="field">
    <%= form.label :student_name %>
    <%= form.text_field :student_name %>
  </div>

  <div class="field">
    <%= form.label :date_of_birth %>
    <%= form.text_field :date_of_birth %>
  </div>

  <div class="field">
    <%= form.label :address %>
    <%= form.text_field :address %>
  </div>

  <div class="field">
    <%= form.label :gmail %>
    <%= form.text_field :gmail %>
  </div>

  <div class="field">
    <%= form.label :phone %>
    <%= form.text_field :phone %>
  </div>

  <div class="actions">
    <%= form.submit 'Save' %>
  </div>
<% end %>
</form>








